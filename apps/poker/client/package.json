  "scripts": {
    "dev": "yarn concurrently -n WEBPACK,SERVE -s command-SERVE \"yarn dev:webpack\" \"yarn serve\"",
    "dev:webpack": "dotenv -e $PROJECT_CWD/env/public/.env.$FLEX_MODE webpack --config webpack.config.js --mode $FLEX_MODE -- --watch",
    "build": "dotenv -e $PROJECT_CWD/env/public/.env.$FLEX_MODE yarn webpack --config webpack.config.js --mode $FLEX_MODE --stats-error-details",
    "serve": "dotenv -e $PROJECT_CWD/env/public/.env.$FLEX_MODE yarn serve:express",
    "serve:express": "echo \"$FLEX_SERVER_LAUNCHING $FLEX_POKER_CLIENT_HOST\n\" && yarn node server.cjs"
  },
