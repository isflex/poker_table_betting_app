{
  "name": "@flexiness/poker_table_betting_app",
  "version": "1.0.0",
  "description": "Poker Table Betting App",
  "main": "index.js",
  "repository": "git@github.com:isflex/poker_table_betting_app.git",
  "author": "ischerer <hello@flexiness.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "resolutions": {
    "pointingpoker-api": "workspace:./apps/poker/api",
    "pointingpoker-client": "workspace:./apps/poker/client",
    "pointingpoker-common": "workspace:./apps/poker/common",
    "pointingpoker-server": "workspace:./apps/poker/server",
    "pointingpoker-parent": "workspace:./apps/poker",
    "@flexiness/domain-utils": "workspace:./packages/flex/domain-utils",
    "@flexiness/tailwind": "workspace:./packages/flex/domain-tailwind",
    "flex-tailwind": "link:./packages/flex/domain-tailwind/dist",
    "@flexiness/eslint-config-flex": "workspace:./packages/flex/config/eslint",
    "@flexiness/certs": "workspace:./packages/flex/config/certs",
    "@flexiness/webpack": "workspace:./packages/flex/config/webpack",
    "flex-webpack": "link:./packages/flex/config/webpack/src",
    "@flex-design-system/framework": "workspace:./packages/flex/design-system-framework",
    "flex-design-system-framework": "link:./packages/flex/design-system-framework/src/modules3/default",
    "flex-design-system-framework/all.module.scss": "link:./packages/flex/design-system-framework/src/modules3/default/all.module.scss",
    "@flex-design-system/react-ts": "workspace:./packages/flex/design-system-react-ts",
    "flex-design-system-react-ts": "link:./packages/flex/design-system-react-ts/src",
    "flex_design_system_react_ts_styled": "link:./packages/flex/design-system-react-ts/src/styled",
    "@flexiness/domain-lib-mobx-react-router": "workspace:./packages/flex/domain-lib-mobx-react-router",
    "@flexiness/aws": "workspace:./packages/flex/config/aws",
    "mini-css-extract-plugin": "2.7.6"
  },
  "scripts": {
    "compile": "yarn clean:tsc && yarn compile:tsc",
    "compile:tsc": "yarn tsc -b ./tsconfig.build.json",
    "compile:tsc:watch": "yarn tsc -b ./tsconfig.build.json --watch",
    "clean:tsc": "rimraf '{./apps/poker/**/dist,./apps/poker/**/*.tsbuildinfo}'",
    "dev": "yarn concurrently -k -n COMPILE,ASSETS,POKER -s first \"yarn compile:tsc:watch\" \"yarn assets:dev\" \"yarn workspace pointingpoker-parent dev\"",
    "assets:dev": "yarn concurrently -k -n CONTENT,TAILWIND -s first \"yarn workspace @flexiness/domain-content start\" \"yarn workspace @flexiness/domain-tailwind dev\"",
    "build": "yarn compile && yarn build:packages && yarn workspace pointingpoker-client build",
    "build:packages": "yarn nx run-many --target=build --projects=\"packages/**/*\" --nx-bail=true",
    "start": "yarn concurrently -k -n ASSETS,POKER -s first \"yarn assets:start\" \"yarn workspace pointingpoker-parent start\"",
    "assets:start": "yarn concurrently -k -n CONTENT -s first \"yarn workspace @flexiness/domain-content start\""
  },
  "devDependencies": {
    "@aws-amplify/api": "5.4.4",
    "@aws-amplify/api-graphql": "3.4.10",
    "@aws-amplify/auth": "5.6.4",
