    "eslint-import-resolver-lerna": "2.0.0",
    "eslint-import-resolver-typescript": "3.2.5",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-i18n-json": "4.0.0",
