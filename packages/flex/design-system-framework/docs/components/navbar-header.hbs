            <div class="navbar-extras is-hidden-touch"></div>
            <div class="navbar-end">
                <div class="navbar-icons">
                    <!-- liens directs -->
                    <!-- lien alternatif dédié au login / espace membre -->
                </div>
            </div>
        </div>
    </nav>
    {{/content}}
    {{/snippet}}
</section>

