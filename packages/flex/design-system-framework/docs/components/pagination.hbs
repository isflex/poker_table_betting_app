---
title: Pagination
active: {
    components: {
        pagination: true
    }
}
root: ../
---
{{#extend "menu-page"}}
{{#content "content"}}
{{#> components-title desc="Pagination"}}{{/components-title }}

<section class="section">
    {{#> snippet title="Pagination"}}
    {{#content "light"}}
    <nav class="pagination is-rounded is-centered" role="navigation" aria-label="pagination">
        <a class="pagination-previous"><span class="flexi-ui-arrow-left-r"></span></a>
        <ul class="pagination-list">
            <li><span class="pagination-ellipsis">&hellip;</span></li>
            <li><a class="pagination-link" aria-label="Aller à la page 45">45</a></li>
            <li><a class="pagination-link is-current" aria-label="Aller à la page 46" aria-current="page">46</a></li>
            <li><a class="pagination-link" aria-label="Aller à la page 47">47</a></li>
            <li><a class="pagination-link" aria-label="Aller à la page 48">48</a></li>
            <li><a class="pagination-link" aria-label="Aller à la page 49">49</a></li>
            <li><span class="pagination-ellipsis">&hellip;</span></li>
        </ul>
        <a class="pagination-next"><span class="flexi-ui-arrow-right-r"></span></a>
    </nav>
    {{/content}}
    {{/snippet}}
</section>

{{/content}}
{{/extend}}
