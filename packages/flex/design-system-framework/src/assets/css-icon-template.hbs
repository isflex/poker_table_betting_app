@font-face {
    font-family: '{{fontName}}';
    src: {{{src}}};
    font-style: normal;
    font-weight: normal;
}

.icon [class^="{{classPrefix}}"]{{#if extraClass}}.{{extraClass}}{{/if}}::before,
.icon [class*=" {{classPrefix}}"]{{#if extraClass}}.{{extraClass}}{{/if}}::before {
    font-family: {{fontName}};
    font-style: normal;
    font-weight: normal;
    display: inline-block;
    text-decoration: inherit;
    margin-right: .2em;
    text-align: center;
    font-variant: normal;
    text-transform: none;
    line-height: 1em;
    margin-left: .2em;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}{{#if extraClass}}.{{extraClass}}{{/if}}::before {
    content: "\\{{this}}";
}

{{/each}}
