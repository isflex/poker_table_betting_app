    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "16.13.1",
    "react-native": "^0.71.8",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "0.13.9",
    "rimraf": "^3.0.2",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "stream-browserify": "3.0.0",
    "style-loader": "3.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.7.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
