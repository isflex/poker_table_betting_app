{
  "name": "@flex-design-system/react-ts",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "Flexiness <hello@flexiness.com>",
  "license": "UNLICENSED",
  "main": "dist/styled",
  "directories": {
    "dist": "dist",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "dist",
    "lib",
    "native"
  ],
  "scripts": {
    "flex-bake": "echo $(pwd)",
    "flex-css": "echo $(pwd)",
    "android": "npm run uncomment && react-native run-android",
    "ios": "npm run uncomment && react-native run-ios --simulator",
    "start:native": "npm run uncomment && react-native start",
    "cp:assets": "rm -rf ./dist/assets && cp -R ./src/assets ./dist/ && cp ./src/views/*.css ./dist/views/",
    "prepack": "npm run build",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --color --quiet --cache",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "native": "rm -rf native && tsc -p tsconfig.native.json",
    "comment": "sed -i -e 's/^/\\/\\/ /' node_modules/react-native/index.js",
    "uncomment": "sed -i -e 's/^\\/\\/ //' node_modules/react-native/index.js",
    "start:web": "env-cmd -f $PROJECT_CWD/env/public/.env.$FLEX_MODE yarn start:os",
    "start:os": "if-env OS=Windows_NT && yarn start:flex-design-system-react-ts-web:windows || yarn start:flex-design-system-react-ts-web",
    "start:flex-design-system-react-ts-web": "webpack serve --config webpack.web.config.js --port $FLEX_DESIGN_SYS_REACT_TS_PORT_WEB_ONLY",
    "start:flex-design-system-react-ts-web:windows": "webpack serve --config webpack.web.config.js --port %FLEX_DESIGN_SYS_REACT_TS_PORT_WEB_ONLY%",
    "pre:start": "echo \"$FLEX_SERVER_LAUNCHING $FLEX_DESIGN_SYS_REACT_TS_HOST\" && yarn start",
    "start": "env-cmd -f $PROJECT_CWD/env/public/.env.$FLEX_MODE concurrently \"yarn webpack\" \"yarn serve:express\"",
    "webpack": "webpack --watch --config webpack.config.js",
    "serve:dev": "serve build -p $FLEX_DESIGN_SYS_REACT_TS_PORT --cors 2>&1",
    "serve:prod": "echo \"$FLEX_SERVER_LAUNCHING $FLEX_DESIGN_SYS_REACT_TS_HOST\n\" && serve build -l tcp://$FLEX_DESIGN_SYS_REACT_TS_HOSTNAME:$FLEX_DESIGN_SYS_REACT_TS_PORT --cors 2>&1",
    "serve:dockerized": "echo \"$FLEX_SERVER_LAUNCHING $FLEX_DESIGN_SYS_REACT_TS_HOST\n\" && yarn serve:dev",
    "serve:express": "echo \"$FLEX_SERVER_LAUNCHING $FLEX_DESIGN_SYS_REACT_TS_HOST\n\" && node server.js",
    "substitute-nx-project": "env-cmd -f $PROJECT_CWD/env/public/.env.$FLEX_MODE envsub --env FLEX_DESIGN_SYS_REACT_TS_HOST --env FLEX_SERVER_RUNNING --syntax dollar-both ./project_template.json ./project.json",
    "serve": "yarn serve:express",
    "launch": "env-cmd -f $PROJECT_CWD/env/public/.env.$FLEX_MODE yarn serve",
    "build:framework:dev": "yarn cp:assets && yarn build:dev",
    "build": "env-cmd -f $PROJECT_CWD/env/public/.env.$FLEX_MODE webpack --config webpack.config.js --mode production",
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "yarn clean:source && yarn clean:built",
    "clean:source:no:js": "rimraf '{src/**/*.d.ts,src/**/*.js,src/**/*.jsx,src/**/*.js.map,src/**/*.jsx.map}'",
    "clean:source": "rimraf '{src/**/*.d.ts}'",
    "clean:built": "rimraf '{build,dist,*.tsbuildinfo,node_modules/.cache/webpack,node_modules/.cache/babel-loader}'",
    "clean:tsc:buildinfo": "rimraf '{*.tsbuildinfo}'"
  },
  "dependencies": {
    "@bytel/trilogy-slider": "link:@bytel/trilogy-slider",
    "@flex-design-system/framework": "link:@flex-design-system/framework",
    "@flexiness/certs": "link:@flexiness/certs",
    "@flexiness/domain-lib-mobx-react-router": "link:@flexiness/domain-lib-mobx-react-router",
    "@react-native-picker/picker": "2.4.2",
    "classnames": "^2.3.1",
    "env-cmd": "^10.1.0",
    "envsub": "4.1.0",
    "flex-design-system-framework": "link:../design-system-framework/src/modules3/default",
    "flex-design-system-react-ts": "link:./src",
    "flex-gateway-ssr": "link:../../gateway/nextjs-telenko/src",
    "json-server": "^0.17.0",
    "lodash": "4.17.21",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "nanoid": "4.0.0",
    "nodemon": "2.0.20",
    "react-native-check-box": "^2.1.7",
    "react-native-circular-progress": "^1.3.7",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-shadow-root": "6.2.0",
    "serve": "14.0.1",
    "tslib": "^2.4.0"
  },
