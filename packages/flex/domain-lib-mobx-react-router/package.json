{
  "name": "@flexiness/domain-lib-mobx-react-router",
  "version": "0.0.2",
  "description": "Mobx React Router library for micro frontend development",
  "keywords": [],
  "author": "Flexiness",
  "license": "MIT",
  "main": "dist/FlexMobxReactRouter",
  "browser": "lib/FlexMobxReactRouter",
  "module": "dist/FlexMobxReactRouter",
  "files": [
    "dist",
    "lib",
    "module"
  ],
  "scripts": {
    "rollup:lib": "dotenv -e $PROJECT_CWD/env/public/.env.$FLEX_MODE yarn rollup -c rollup.config.js",
    "tsc:lib": "tsc -p ./tsconfig.json",
    "clean:lib": "yarn clean:source && yarn clean:built",
    "clean:source": "rimraf '{src/**/*.d.ts,src/**/*.js,src/**/*.js.map}'",
    "clean:built": "rimraf '{lib,module,dist,*.tsbuildinfo}'",
    "clean:tsc:buildinfo": "rimraf '{*.tsbuildinfo}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --color --quiet --cache",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "patch:rollup:lib": "npm version patch"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime-corejs3": "7.18.6",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/node": "^18.0.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "flexiness": "link:@types/flexiness",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "history": "^5",
    "mobx": "^6",
    "path-to-regexp": "^6",
    "react-router": "^6",
    "react-router-dom": "^6"
  },
  "dependencies": {
    "@superwf/mobx-react-router": "^7.4.0",
    "dotenv-cli": "7.3.0",
    "history": "^5.3.0",
    "loglevel": "1.8.0",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.3.0",
    "universal-router": "9.1.0"
  }
}
